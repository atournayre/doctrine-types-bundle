{
    "name": "atournayre/doctrine-types-bundle",
    "description": "This bundle adds Doctrine Types.",
    "type": "symfony-bundle",
    "license": [ "MIT" ],
    "authors": [
        {
            "name": "atournayre",
            "email": "aurelien.tournayre@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Atournayre\\Bundle\\DoctrineTypes\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "atournayre/types": "^0",
        "symfony/flex": "^2.2",
        "doctrine/dbal": "^3.6",
        "symfony/dependency-injection": "6.2.*",
        "symfony/config": "6.2.*",
        "symfony/http-kernel": "6.2.*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.2.*",
            "endpoint": [
                "https://api.github.com/repos/atournayre/symfony-flex-recipes/contents/index.json",
                "flex://defaults"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
